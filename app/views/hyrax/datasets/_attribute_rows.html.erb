<%#= presenter.attribute_to_html(:contributor_nested, render_as: :nested_contributor, label: I18n.t('ebsco.fields.contributor_nested')) %>
<%= presenter.attribute_to_html(:identifier_nested, render_as: :nested_identifier, label: I18n.t('ebsco.fields.identifier_nested')) %>
<%#= presenter.attribute_to_html(:alt_identifier_nested, render_as: :nested_alt_identifier, label: I18n.t('ebsco.fields.alt_identifier_nested')) %>
<%#= presenter.attribute_to_html(:rights_nested, render_as: :nested_rights, label: I18n.t('ebsco.fields.rights_nested')) %>
<%= presenter.attribute_to_html(:description_nested, render_as: :nested_description, label: I18n.t('ebsco.fields.description_nested')) %>
<%= presenter.attribute_to_html(:subject_nested, render_as: :nested_subject, label: I18n.t('ebsco.fields.subject_nested')) %>
<%= presenter.attribute_to_html(:funding_nested, render_as: :nested_funding, label: I18n.t('ebsco.fields.funding_nested')) %>
<%= presenter.attribute_to_html(:geolocation_nested, render_as: :nested_geolocation, label: I18n.t('ebsco.fields.geolocation_nested')) %>
<%= presenter.attribute_to_html(:related_url, render_as: :related_url_links, label: I18n.t('ebsco.fields.related_url')) %>
<%# if current_user.roles.pluck(:name).include?('reviewer') %>
  <%#= presenter.attribute_to_html(:work_review_process, render_as: :nested_work_review_process, label: I18n.t('ebsco.fields.work_review_process')) %>
<%#end%>
<% if !user_roles(current_user, 'user') %>
  <%= presenter.attribute_to_html(:reviewers, render_as: :nested_reviewers, label: I18n.t('ebsco.fields.reviewers')) %>
<%end%>
<%= presenter.attribute_to_html(:related_identifier_nested, render_as: :nested_related_identifier, label: I18n.t('ebsco.fields.related_identifier_nested')) %>
<%#= presenter.attribute_to_html(:date_nested, render_as: :nested_date, label: I18n.t('ebsco.fields.date_nested')) %>
<% if user_roles(current_user, 'reviewer') %>
  <ul class="tabular">
    <li class="attribute attribute-add-review">
      <a href="<%= request.base_url + "/concern/work_reviews/new?work_review_creator=#{current_user.display_name}&work_review_title=#{presenter.title.first}&dataset_id=#{params[:id]}&locale=en" %>" class="btn btn-primary" target="_blank">Add Review</a>
    </li>
  </ul><br>
<%end%>

