<%= presenter.attribute_to_html(:other_title, render_as: :other_title, label: I18n.t('ebsco.fields.other_title')) %>
<%= presenter.attribute_to_html(:publisher, render_as: :faceted) %>
<%= presenter.attribute_to_html(:language, render_as: :faceted) %>
<%= presenter.attribute_to_html(:publication_year, render_as: :publication_year, label: I18n.t('ebsco.fields.publication_year')) %>
<%= presenter.attribute_to_html(:dataset_size, render_as: :dataset_size, label: I18n.t('ebsco.fields.dataset_size')) %>
<%= presenter.attribute_to_html(:dataset_format, render_as: :dataset_format, label: I18n.t('ebsco.fields.dataset_format')) %>
<%= presenter.attribute_to_html(:dataset_version, render_as: :dataset_version, label: I18n.t('ebsco.fields.dataset_version')) %>
